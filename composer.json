{
  "name": "sabbir/larashield",
  "description": "User, Role, Auth management package with ResponseBuilder and Policies",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Larashield\\": "src/"
    }
  },
  "authors": [
    { "name": "SabbirHR", "email": "sabbirhossen860@gmail.com" }
  ],
  "require": {
